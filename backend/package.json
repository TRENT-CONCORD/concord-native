{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for Concord application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node src/index.ts",
    "migrate:dev": "cross-env NODE_ENV=development npm run typeorm migration:run -- -d dist/cli-config.js",
    "migrate:prod": "cross-env NODE_ENV=production npm run typeorm migration:run -- -d dist/cli-config.js",
    "test:connection": "ts-node src/test-env-connection.ts",
    "build": "tsc",
    "typeorm": "typeorm",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -d dist/cli-config.js",
    "migration:run": "npm run build && npm run typeorm migration:run -- -d dist/cli-config.js",
    "migration:revert": "npm run build && npm run typeorm migration:revert -- -d dist/cli-config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "pg": "^8.13.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.11",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "tsconfig-paths": "^4.2.0"
  }
}
